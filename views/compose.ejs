<%- include('partials/header'); %>
<h1>Compose</h1>
<form action="#" method="post" autocomplete="off">
    <div class="form-group">
        <label>Title</label>
        <input name="postTitle" type="text" class="form-control" autofocus>
    </div>
    <div class="form-group">
        <label>Post</label>
        <textarea type="text" name="postContent" cols="40" rows="4" class="form-control"></textarea>
    </div>
    <button class="btn btn-primary" type="submit">Publish</button>
</form>
<%- include('partials/footer'); %>